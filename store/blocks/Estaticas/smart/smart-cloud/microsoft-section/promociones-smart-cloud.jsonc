{
        /******* Promociones smart cloud ********/
    
        "disclosure-layout#promociones-smart-cloud":{
            "children": ["disclosure-trigger#promociones-smart-cloud", "disclosure-content#promociones-smart-cloud"]
         }, 
         "disclosure-trigger#promociones-smart-cloud":{
           "children": ["rich-text#promociones-smart-cloud"],
           "props":{
               "blockClass":["sub-btn-trigger-smart", "sub-btn-trigger-lvl1"]
           }
         },
         "disclosure-content#promociones-smart-cloud": {
           "children": [ "flex-layout.row#promociones-smart-cloud"],
           "props":{
               "blockClass":["sub-content-disclosure-smart","sub-content-lvl1"]
           }
         },
          "flex-layout.row#promociones-smart-cloud":{
            "children":["flex-layout.col#promociones-smart-cloud"],
            "props":{
                "blockClass":["sub-disclosure","promociones-smart-cloud"]
            }
        },
         "flex-layout.col#promociones-smart-cloud":{
            "children":[
                "rich-text#promociones-smart-cloud-1",
                "flex-layout.row#promociones-productos",
                "link#promociones"
            ],
            "props":{
                "blockClass":["col-cloud","col-cloud-microsoft"]
            }
        },
        "rich-text#promociones-smart-cloud": {
            "props": {
                "text": " ## Promociones smart cloud",
                "blockClass": ["subtitle","subtitle-portafolio-servicios","subtitle-azul"]
            }
        },
        "rich-text#promociones-smart-cloud-1": {
            "props": {
                "text": "Cada licencia Office 365 E1 contiene las siguientes aplicaciones:",
                "blockClass": ["cloud-text","promociones-smart-cloud-1"]
            }
        },
        "flex-layout.row#promociones-productos":{
          "children":[
            "image#planner",
            "image#sharepoint",
            "image#onedrive",
            "image#yammer",
            "image#teams",
            "image#powerapps",
            "image#flow",
            "image#exchange"
        ],
          "props":{
              "blockClass":["sub-disclosure","promociones-smart-cloud"]
          }
      },
      "image#planner":{
        "props":{
          "src": "https://orbeim.vteximg.com.br/arquivos/icon-microsoft-planner.png",
          "link":"#",
          "title":"Microsoft Planner"
        }
      },
      "image#sharepoint":{
        "props":{
          "src": "https://orbeim.vteximg.com.br/arquivos/icon-microsoft-sharepoint.png",
